<div class="row">
	<div class="col-md-9 bottom-padded-col">
		<% @scratchpads.each do |sp| %>
			<article>
				<h3><strong><%= sp.title %></strong></h3>
				<h6><%= sp.created_at.strftime("%m/%d/%Y") %></h6>
				<p><%= raw sp.preview %></p>
				<%= link_to 'View', sp %>
			</article>
		<% end %>
	</div>
</div>